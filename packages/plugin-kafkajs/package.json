{
  "name": "@aspecto/plugin-kafkajs",
  "version": "0.0.1",
  "description": "open telemetry instrumentation for the `kafkajs` kafka client",
  "keywords": [
    "kafka",
    "opentelemetry",
    "kafkajs"
  ],
  "author": "Amir Blum <amir@aspecto.io>",
  "homepage": "https://github.com/aspecto-io/opentelemetry-ext-js",
  "license": "Apache-2.0",
  "main": "dist/src/index.js",
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/aspecto-io/opentelemetry-ext-js.git"
  },
  "scripts": {
    "build": "tsc",
    "prepare": "yarn run build",
    "test": "jest",
    "watch": "tsc -w",
    "prettier": "prettier --config .prettierrc --write \"src/**/*.{ts,tsx,js,jsx,json}\""
  },
  "bugs": {
    "url": "https://github.com/aspecto-io/opentelemetry-ext-js/issues"
  },
  "devDependencies": {
    "@opentelemetry/node": "^0.8.3",
    "@opentelemetry/tracing": "^0.8.3",
    "@types/jest": "^25.2.3",
    "@types/shimmer": "^1.0.1",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "kafkajs": "^1.12.0",
    "prettier": "2.0.5",
    "ts-jest": "^26.1.0",
    "typescript": "^3.9.5"
  },
  "peerDependencies": {
    "kafkajs": "^1.12.0"
  },
  "dependencies": {
    "@opentelemetry/core": "^0.8.3",
    "shimmer": "^1.2.1"
  },
  "jest": {
    "preset": "ts-jest",
    "globals": {
      "ts-jest": {
        "tsConfig": "./tsconfig.json"
      }
    },
    "testMatch": [
      "**/test/**/*.spec.+(ts|tsx)"
    ],
    "testEnvironment": "node"
  },
  "husky": {
    "hooks": {
        "pre-push": "yarn test"
    }
  }
}
